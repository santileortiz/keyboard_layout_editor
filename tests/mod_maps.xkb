    key <RWIN> { [ Super_R ] };
    key <META> { [ Meta_L ] };
    key <SUPR> { [ Super_L ] };
    key <HYPR> { [ Hyper_L ] };

    modifier_map Control { <RCTL> };
    modifier_map Control { <LCTL> };
    modifier_map Shift { <LFSH> };
    modifier_map Shift { <RTSH> };
    modifier_map Lock { <CAPS> };
    modifier_map Mod1 { <RALT> };
