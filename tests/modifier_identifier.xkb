xkb_types "mod_ident_t" {

    virtual_modifiers NumLock,Alt,LevelThree,LAlt,RAlt,RControl,LControl,ScrollLock,LevelFive,AltGr,Meta,Super,Hyper;

    type "ONE_LEVEL" {
        modifiers= none;
        level_name[Level1]= "Any";
    };
    type "TWO_LEVEL" {
        modifiers= Shift;
        map[Shift]= Level2;
        level_name[Level1]= "Base";
        level_name[Level2]= "Shift";
    };
    type "ALPHABETIC" {
        modifiers= Shift+Lock;
        map[Shift]= Level2;
        map[Lock]= Level2;
        level_name[Level1]= "Base";
        level_name[Level2]= "Caps";
    };
    type "KEYPAD" {
        modifiers= Shift+NumLock;
        map[Shift]= Level2;
        map[NumLock]= Level2;
        level_name[Level1]= "Base";
        level_name[Level2]= "Number";
    };
    type "MOD_TESTING_TYPE_A" {
        modifiers= Shift+Lock+Control+Mod1;
        map[Shift]= Level2;
        map[Lock]= Level3;
        map[Control]= Level4;
        map[Mod1]= Level5;
        level_name[Level1]= "Base";
        level_name[Level2]= "Shift";
        level_name[Level3]= "Lock";
        level_name[Level4]= "Control";
        level_name[Level5]= "Mod1";
    };
    type "MOD_TESTING_TYPE_B" {
        modifiers= Mod2+Mod3+Mod4+Mod5;
        map[Mod2]= Level2;
        map[Mod3]= Level3;
        map[Mod4]= Level4;
        map[Mod5]= Level5;
        level_name[Level1]= "Base";
        level_name[Level2]= "Mod2";
        level_name[Level3]= "Mod3";
        level_name[Level4]= "Mod4";
        level_name[Level5]= "Mod5";
    };
};

xkb_compatibility "mod_ident_c" {
    indicator "Caps Lock" {
        !allowExplicit;
        whichModState= locked;
        modifiers= Lock;
    };
    indicator "Num Lock" {
        !allowExplicit;
        whichModState= locked;
        modifiers= NumLock;
    };
};

